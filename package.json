{
  "name": "babel-tester",
  "version": "0.0.1",
  "description": "An opinionated library to make testing Babel plugins easier with Jest",
  "keywords": [
    "babel",
    "jest",
    "test"
  ],
  "main": "src/index.js",
  "files": [
    "src/"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satya164/babel-tester.git"
  },
  "author": "Satyajit Sahoo <satyajit.happy@gmail.com> (https://github.com/satya164/)",
  "scripts": {
    "lint": "eslint .",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "codecov": "^3.1.0",
    "eslint": "^5.9.0",
    "eslint-config-satya164": "^2.0.1",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "jest-file-snapshot": "^0.2.1",
    "prettier": "^1.15.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "watchPathIgnorePatterns": [
      "__fixtures__\\/[^/]+\\/(output|error)\\.js"
    ]
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "error-stack-parser": "^2.0.2",
    "escape-string-regexp": "^1.0.5",
    "strip-ansi": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  }
}
